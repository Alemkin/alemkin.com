doctype html
html
  head
    != partial("partial/_main_header")
    link(rel="stylesheet" href="/css/base.css")
    link(rel="canonical" href="https://www.alemkin.com/")

  body
    div(class="object")
      p const
        span(class="name")  Alexander Lemkin
        span  =
        span  {
        div(class="inner-object")
          p
            span(class="far fa-clipboard code-link-icon green")
            span(class="object-title") resum&#233;:
            a(href="/resume" target="_blank" class="code-link") alemkin.com/resume
          p
            span(class="fab fa-git code-link-icon white")
            span(class="object-title") github:
            a(href="https://www.github.com/alemkin" target="_blank" class="code-link") github.com/alemkin
          p
            span(class="fab fa-linkedin-in code-link-icon blue")
            span(class="object-title") linkedIn:
            a(href="https://www.linkedin.com/pub/alexander-lemkin/49/637/636" target="_blank" class="code-link") li/alexander-lemkin
          p
            span(class="fab fa-goodreads-g code-link-icon brown")
            span(class="object-title") goodreads:
            a(href="http://www.goodreads.com/user/show/18097559-alexander-lemkin" target="_blank" class="code-link") gr/alexander-lemkin
      p }